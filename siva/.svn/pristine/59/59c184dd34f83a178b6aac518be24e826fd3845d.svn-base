public class PassFail {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter your score in exam");
		int score=sc.nextInt();
		boolean result=hasPassed(score);
		if(result== true)
		System.out.println("Passed Successfully!");
		else
System.out.println("Failed! The range of the qualifying exam marks is between 65 and 100");

		
	}
 public static boolean hasPassed(int marks)
{
	if(marks>=65 && marks<=100)
	return true;
	else 
	return false;
	}
}
