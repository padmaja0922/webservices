package com.sivakiran.eliminationtest.database;

public class ProductDb {
	public String phn;
	public String wth;
	public String pc;
	public String software;
	public static ProductDb displayProducts()
	{
		ProductDb prdt=new ProductDb();
		prdt.phn="i-phone";
		prdt.wth="Rolex";
		prdt.pc="Dell";
		prdt.software="Windows8";
		return prdt;
	}
	public String getPhn() {
		return phn;
	}
	public String getWth() {
		return wth;
	}
	public String getPc() {
		return pc;
	}
	public String getSoftware() {
		return software;
	}

}
